@foreach (var usg in Model.Usings)
{
@:using @usg;
}

namespace @Model.Namespace;
public interface @Model.InterfaceName
{
    Task<PagedResponse<@Model.ModelName>> GetManyAsync(QueryParameters parameters, CancellationToken token);

    Task<@Model.ModelName> GetByIdAsync(@Model.KeyType id, CancellationToken token);

    Task CreateAsync(@Model.CommandModelName model, CancellationToken token);

    Task UpdateAsync(@Model.CommandModelName model, CancellationToken token);

    Task DeleteAsync(@Model.KeyType id, CancellationToken token);
}