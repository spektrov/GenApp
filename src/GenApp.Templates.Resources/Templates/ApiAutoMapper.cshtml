@foreach (var usg in Model.Usings)
{
@:using @usg;
}

namespace @Model.Namespace;
public class Mapper : Profile
{
    public Mapper()
    {
        @for (int i = 0; i < Model.Models.Count; i++)
        {
        @:CreateMap<@Model.Models[i].CreateRequestName, @Model.Models[i].CommandModelName>();
        @:CreateMap<@Model.Models[i].UpdateRequestName, @Model.Models[i].CommandModelName>();
        @:CreateMap<@Model.Models[i].ModelName, @Model.Models[i].ResponseName>();
            if (i != Model.Models.Count - 1)
            {
        @:
            }
        }
    }
}